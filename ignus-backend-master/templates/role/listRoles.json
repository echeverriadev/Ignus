{
  "status": true,
  "data": {
      "{{#each roles}}": {
        "id": "{{#? this.id}}",
        "name": "{{#? this.name}}",
        "description": "{{#? this.description}}",
        "functions" : {
          "{{#each this.Functions}}": {
            "id": "{{#? this.id}}",
            "name": "{{#? this.name}}",
            "description": "{{#? this.description}}"
          }
        }
      }
  }
}